<h1>Create Product</h1>
<%= form_for @product do |f| %>
  <div>
    <%= f.label :product_name %>
    <%= f.text_field :product_name %>
  </div>
  <div>
    <%= f.label :product_quantity %>
    <%= f.text_field :product_quantity %>
  </div>
  <div>
    <%= f.label :product_weight%>
    <%= f.text_field :product_weight %>
  </div>
  <div>
    <%= f.label :product_dimension %>
    <%= f.text_field :product_dimension %>
  </div>
  <div>
    <%= f.label :product_description %>
    <%= f.text_field :product_description %>
  </div>
  <div>
    <%= f.label :product_image_url %>
    <%= f.text_field :product_image_url %>
  </div>
  <div>
    <%= f.label :product_group %>
    <%= f.text_field :product_group %>
  </div>
  <div>
    <%= f.label :product_price %>
    <%= f.text_field :product_price %>
  </div>


  <div>
    <%= f.submit "Done" %>
  </div>
<% end %>
