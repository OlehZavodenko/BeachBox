
<div class="panel panel-default">
  <div class="panel-heading" >
    <h4 class="panel-title">
      <div data-toggle="" data-parent="#accordion" href="#collapse3">
        Here's what you need...</div>
      </h4>
    </div>

    <!-- Panel 3 body -->
    <div  class="panel-collapse collapse" id="collapse3">
      <div class="panel-body">


        <div class="product-list">

          <% @products.each do |product| %>

          <%= render "product_row", product: product, order_item: @order_item %>
          <% end %>
        </div>



        
      </div>
      <span data-tooltip="  Changes will be lost.">
        <button type="button" class="btn btn-primary" id="back-to-2">Back</button>
      </span>
      <%= link_to '<button type="button" class="btn btn-primary cart-update">Next</button>'.html_safe, '/delivery' %>
    </div>

  </div>
